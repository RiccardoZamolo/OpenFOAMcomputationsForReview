FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState Boussinesq;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        molWeight       0; // does not enter
    }
    thermodynamics
    {
        Cp              1;
        Hf              0;
    }
    transport
    {
        mu              0.71;
        Pr              0.71;
    }
    equationOfState
    {
      rho0    1; // does not enter
      T0    0.5; // does not enter
      beta 0.71; // does not enter
    }
    
}


