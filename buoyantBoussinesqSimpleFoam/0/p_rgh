FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    "(inner|outer)"
    {
        type            fixedFluxPressure;
        rho             1.0;
        value           uniform 0;
    }

    "(frontPlane|backPlane)"
    {
        type            wedge;
    }
}

